<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>slash.navigation</groupId>
    <artifactId>route-converter</artifactId>
    <packaging>jar</packaging>
    <name>The UI you see if you start the program</name>
    <parent>
        <groupId>slash.navigation</groupId>
        <artifactId>RouteConverter</artifactId>
        <version>1.34-SNAPSHOT</version>
    </parent>

    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>native2ascii-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <goals>
                            <goal>native2ascii</goal>
                        </goals>
                        <configuration>
                            <encoding>UTF8</encoding>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
    
    <dependencies>
        <dependency>
            <groupId>slash.navigation</groupId>
            <artifactId>catalog</artifactId>
            <version>1.34-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>slash.navigation</groupId>
            <artifactId>common</artifactId>
            <version>1.34-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>slash.navigation</groupId>
            <artifactId>common-gui</artifactId>
            <version>1.34-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>slash.navigation</groupId>
            <artifactId>earthtools</artifactId>
            <version>1.34-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>slash.navigation</groupId>
            <artifactId>elevationview</artifactId>
            <version>1.34-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>slash.navigation</groupId>
            <artifactId>geonames</artifactId>
            <version>1.34-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>slash.navigation</groupId>
            <artifactId>googlemaps</artifactId>
            <version>1.34-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>slash.navigation</groupId>
            <artifactId>hgt</artifactId>
            <version>1.34-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>slash.navigation</groupId>
            <artifactId>mapview</artifactId>
            <version>1.34-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>slash.navigation</groupId>
            <artifactId>swt-mapview</artifactId>
            <version>1.34-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>com.intellij</groupId>
            <artifactId>forms-rt</artifactId>
            <version>6.0.5</version>
            <classifier>stripped</classifier>
        </dependency>
    </dependencies>
</project>
